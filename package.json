{
  "name": "chingu-game-dev",
  "version": "1.0.0",
  "description": "Project Managment Site for the Chingu Game Dev Project",
  "main": "client.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/server",
    "analyzesize": "NODE_ENV=production webpack -p --json --config webpack.config.client.js | webpack-bundle-size-analyzer",
    "build": "rm -rf ./dist && npm run copyassets && npm run build:server && npm run build:client",
    "build:client": "webpack -p --config webpack.config.client.js",
    "build:server": "webpack -p --config webpack.config.server.js",
    "dev": "rm -rf ./dist && npm run copyassets && npm-run-all --parallel dev:server dev:client",
    "dev:server": "webpack --config webpack.config.server.js",
    "dev:client": "webpack-dev-server --config webpack.config.client.js",
    "copyassets": "mkdir -p ./dist/public && cp -r ./assets ./dist/public/assets"
  },
  "dependencies": {
    "@aoberoi/passport-slack": "^1.0.5",
    "autoprefixer": "^6.7.6",
    "axios": "^0.16.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "body-parser": "^1.17.1",
    "css-loader": "^0.26.2",
    "dotenv": "^10.0.0",
    "express": "^4.15.0",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "jeet": "^7.1.0",
    "json-loader": "^0.5.4",
    "mongodb": "3.0.0",
    "mongoose": "5.7.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "passport-mock-strategy": "^2.0.0",
    "postcss-loader": "^1.3.3",
    "preact": "8.5.2",
    "preact-compat": "^3.13.1",
    "pug": "^2.0.0-beta11",
    "react-markdown-loader": "https://github.com/kimkwanka/react-markdown-loader.git",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "session-file-store": "^1.0.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "start-server-webpack-plugin": "^2.1.2",
    "style-loader": "^0.13.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.2.2",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "webpack-dev-server": "^2.4.1"
  }
}
